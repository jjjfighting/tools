{
  "manifest_version": 2,
  "name": "Share-cookies",
  "version": "1.0.0",
  "description": "从测试环境复制粘贴cookies到localhost",
  "icons": {
    "16": "./img/favicon.png",
    "48": "./img/favicon.png",
    "128": "./img/favicon.png"
  },
  "browser_action": {
    "default_icon": "./img/favicon.png",
    "default_title": "复制粘贴cookies",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "cookies",
    "storage",
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ]
}
